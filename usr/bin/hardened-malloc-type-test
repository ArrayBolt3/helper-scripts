if grep -q "libhardened_malloc-light.so" "/proc/$$/maps"; then
    echo "kicksecure"
    exit 0
fi

if grep -q "libhardened_malloc.so" "/proc/$$/maps"; then
    echo "original"
    exit 0
fi

echo "none"
exit 1
